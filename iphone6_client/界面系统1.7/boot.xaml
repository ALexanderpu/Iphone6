<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         xmlns:gifLib="clr-namespace:GifImageLib;assembly=GifImageLib"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d" x:Class="iphone6.boot"
        Title="boot" Height="600" Width="300"
        WindowStyle = "None"
        AllowsTransparency ="True">
    <Window.Background>
        <ImageBrush ImageSource="Images\background1.png"/>
    </Window.Background>
	<Grid d:IsLocked="True">
		<gifLib:GifImage Source="Images\boot1.gif" Stretch="Uniform" d:IsLocked="True"/>
		<Image x:Name="back" Margin="139,550,135,20" Source="Images\go.jpg" MouseLeftButtonDown="back_Click" OpacityMask="Black" RenderTransformOrigin="0.133,0.7" d:IsLocked="True"/>
	</Grid>
</Window>

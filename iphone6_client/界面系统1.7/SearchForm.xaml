<Window x:Class="iphone6.SearchForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SearchForm" Height="600" Width="300"
        WindowStyle="None"
        AllowsTransparency="True">
    <Window.Background>
        <ImageBrush ImageSource="Images\background1.png"></ImageBrush>
    </Window.Background>
    <Grid>
        <ListBox x:Name="listBox1" HorizontalAlignment="Left" Height="323" Margin="32,107,0,0" VerticalAlignment="Top" Width="234"/>
        <Image x:Name="button2" Height="50" Margin="120,455,130,0" VerticalAlignment="Top" Width="50" Source="Images\ash-bin.png" MouseLeftButtonDown="button2_Click" OpacityMask="Black" RenderTransformOrigin="0.133,0.7"/>
        <Image x:Name="button1" Height="50" Margin="122,537,128,0" VerticalAlignment="Top" Width="50" Source="Images\back.png" MouseLeftButtonDown="button1_Click" OpacityMask="Black" RenderTransformOrigin="0.133,0.7"/>

    </Grid>
</Window>

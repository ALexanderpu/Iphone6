<Window x:Class="iphone6.MessageBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MessageBox" Height="194" Width="260"
        WindowStyle = "None"
        AllowsTransparency="True">
    <Window.Background>
        <ImageBrush ImageSource="Images\background2.jpg"></ImageBrush>
    </Window.Background>
    <Grid RenderTransformOrigin="0.042,0.496" Height="197" VerticalAlignment="Top">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="label1" HorizontalAlignment="Left" TextWrapping="Wrap" Text="TextBox" Width="260" Background="#FFC5C5C5" FontSize="16" Margin="0,0,0,50"/>
        <!--<Label x:Name="label1" MaxWidth="260" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" Height="150" Width="260" FontSize="24" Background="#FFBFB6B6" Grid.ColumnSpan="2" />-->
        <!--<Image x:Name="Back" Margin="123,531,4,-381" Source="Images\Tick.png" MouseLeftButtonDown="back_Click" OpacityMask="Black" RenderTransformOrigin="0.133,0.7"/>-->
        <Image x:Name="delete" Height="50" Width="50" Margin="105,147,105,0" VerticalAlignment="Top" Source="Images\ash-bin.png" MouseLeftButtonDown="delete_Click" OpacityMask="Black" RenderTransformOrigin="0.133,0.7"/>


    </Grid>
</Window>
